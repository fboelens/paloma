      96.  00:65B5                      ; FUNCTION:	depack
      97.  00:65B5                      ;	Depack a blob of data that was packed with Bitbuster.
      98.  00:65B5                      ;
      99.  00:65B5                      ; ENTRY:
     100.  00:65B5                      ;	HL - Address of packed data
     101.  00:65B5                      ;	DE - Address to depack to
     102.  00:65B5                      ;   A - Start decompressing new data (0) or continue decompressing (<>0)
     103.  00:65B5                      ;
     104.  00:65B5                      ; EXIT:
     105.  00:65B5                      ;	HL - Size of depacked data
     106.  00:65B5                      ;	A  - Number of blocks left to decompress
     107.  00:65B5                      ;	FZ - Last block has been decompressed
     108.  00:65B5                      ;	FNZ- Last block hasn't been decompressed
     109.  00:65B5                      ;
     110.  00:65B5                      ; MODIFIES:
     111.  00:65B5                      ;	#AF, BC, DE, HL#
     112.  00:65B5                      ;
     113.  00:65B5                      depack:
     114.  00:65B5                      		
     115.  00:65B5  B7                  		or	a
     116.  00:65B6  20 02               		jr	nz,depack_next_block	; continue depacking a block if more blocks left
     117.  00:65B8                      		
     118.  00:65B8  7E                  		ld	a,(hl)		; get the number of blocks
     119.  00:65B9                      		
     120.  00:65B9  23                  		inc	hl			; move to size of first block
     121.  00:65BA                      		
     122.  00:65BA                      depack_next_block:
     123.  00:65BA  23                  		inc	hl
     124.  00:65BB  23                  		inc	hl          	; move over block size
     125.  00:65BC                      
     126.  00:65BC  F5                  		push af				; push number of blocks
     127.  00:65BD  D5                  		push de				; push destination address to be able to calculate decompressed size later on
     128.  00:65BE                      		
     129.  00:65BE  CD CC 65            		call depack_block	; decompress the block
     130.  00:65C1                      
     131.  00:65C1  EB                  		ex de,hl			; DE -> pointer to next block, HL -> pointer to first byte after decompressed data
     132.  00:65C2  C1                  		pop bc				; pop the original destination address
     133.  00:65C3  B7                  		or a				; clear carry
     134.  00:65C4  ED 42               		sbc hl,bc			; calculate length of decompressed data
     135.  00:65C6  44                  		ld b,h
     136.  00:65C7  4D                  		ld c,l				; put it in BC
     137.  00:65C8  EB                  		ex de,hl			; HL -> pointer to next block
     138.  00:65C9                      
     139.  00:65C9  F1                  		pop af				; pop number of blocks
     140.  00:65CA  3D                  		dec a				; one less to decompress
     141.  00:65CB  C9                  		ret
     142.  00:65CC                      
     143.  00:65CC                      
     144.  00:65CC                      ; FUNCTION:	depack_block
     145.  00:65CC                      ;	Depack data that was packed with Bitbuster.
     146.  00:65CC                      ;	Decompresses the RAW data, i.e. the data that is stored after the block count and block size!
     147.  00:65CC                      ;
     148.  00:65CC                      ; ENTRY:
     149.  00:65CC                      ;	HL - Address of packed data
     150.  00:65CC                      ;	DE - Address to depack to
     151.  00:65CC                      ;
     152.  00:65CC                      ; EXIT:
     153.  00:65CC                      ;	HL - Address of first byte after compressed data
     154.  00:65CC                      ;	DE - Address of first byte after decompressed data
     155.  00:65CC                      ;
     156.  00:65CC                      ; MODIFIES:
     157.  00:65CC                      ;	#AF, BC, BC', DE, DE', HL, HL'#
     158.  00:65CC                      ;
     159.  00:65CC                      depack_block:
     160.  00:65CC                      			
     161.  00:65CC                                      
     162.  00:65CC  3E 80                       ld	a,128
     163.  00:65CE                      
     164.  00:65CE                      depack_loop:    GET_BIT_FROM_BITSTREAM		; get compression type bit
     164.  00:65CE  CD AF 65          >   call get_bit_from_bitstream
     165.  00:65D1  38 04               		jp	c,output_compressed	; if set, we got lz77 compression
     166.  00:65D3  ED A0               		ldi				; copy byte from compressed data to destination (literal byte)
     167.  00:65D5                      	
     168.  00:65D5                      		IFDEF	BITBUSTER_OPTIMIZE_SPEED
     169.  00:65D5                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     170.  00:65D5                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     171.  00:65D5                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     172.  00:65D5                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     173.  00:65D5                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     174.  00:65D5                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     175.  00:65D5                    ~ 		ENDIF
     176.  00:65D5                      
     177.  00:65D5  18 F7               		jp	depack_loop
     178.  00:65D7                      	
     179.  00:65D7                      
     180.  00:65D7                      ;handle compressed data
     181.  00:65D7                      output_compressed:
     182.  00:65D7                      ; calculate length value
     183.  00:65D7  01 01 00            		ld	bc,1
     184.  00:65DA                      get_gamma_value:
     185.  00:65DA                      	  	GET_BIT_FROM_BITSTREAM   	; get more bits
     185.  00:65DA  CD AF 65          >   call get_bit_from_bitstream
     186.  00:65DD  30 0A               	  	jr 	nc,get_gamma_value_end
     187.  00:65DF                      	  	GET_BIT_FROM_BITSTREAM   	; get next bit of value from bitstream
     187.  00:65DF  CD AF 65          >   call get_bit_from_bitstream
     188.  00:65E2  CB 11               	  	rl	c
     189.  00:65E4  CB 10               	  	rl	b
     190.  00:65E6  30 F2               	  	jp 	nc,get_gamma_value  	; repeat unless overflow occurred (=end of block marker)
     191.  00:65E8  C9                  	  	ret
     192.  00:65E9                        	
     193.  00:65E9                      get_gamma_value_end:
     194.  00:65E9  FD 60               		ld iyh,b
     195.  00:65EB  FD 69               		ld iyl,c
     196.  00:65ED                      
     197.  00:65ED  4E                  		ld	c,(hl)			; get lowest 7 bits of offset, plus offset extension bit
     198.  00:65EE  23                  		inc	hl			; to next byte in compressed data
     199.  00:65EF                      	  	
     200.  00:65EF  06 00               		ld	b,0
     201.  00:65F1  CB 79               		bit	7,c
     202.  00:65F3  28 16               		jr	z,output_match1		; no need to get extra bits if carry not set
     203.  00:65F5                      	
     204.  00:65F5                      		GET_BIT_FROM_BITSTREAM		; get offset bit 10 
     204.  00:65F5  CD AF 65          >   call get_bit_from_bitstream
     205.  00:65F8  CB 10               		rl	b
     206.  00:65FA                      		GET_BIT_FROM_BITSTREAM		; get offset bit 9
     206.  00:65FA  CD AF 65          >   call get_bit_from_bitstream
     207.  00:65FD  CB 10               		rl	b
     208.  00:65FF                      		GET_BIT_FROM_BITSTREAM		; get offset bit 8
     208.  00:65FF  CD AF 65          >   call get_bit_from_bitstream
     209.  00:6602  CB 10               		rl	b
     210.  00:6604                      		GET_BIT_FROM_BITSTREAM		; get offset bit 7
     210.  00:6604  CD AF 65          >   call get_bit_from_bitstream
     211.  00:6607                      
     212.  00:6607  38 03               		jp	c,output_match2		; since extension mark already makes bit 7 set
     213.  00:6609  CB B9               		res	7,c			; only clear it if the bit should be cleared
     214.  00:660B  37                  output_match1:  scf
     215.  00:660C  E5                  output_match2:	push	hl			; address compressed data on stack
     216.  00:660D                      
     217.  00:660D  62                  		ld	h,d
     218.  00:660E  6B                  		ld	l,e			; destination address in HL...
     219.  00:660F  ED 42               		sbc	hl,bc		; calculate source address, keep the required CF in mind! (length was stored length-2)
     220.  00:6611                      
     221.  00:6611  FD 44               		ld b,iyh
     222.  00:6613  FD 4D               		ld c,iyl
     223.  00:6615                      	
     224.  00:6615  ED B0               		ldir			; transfer data
     225.  00:6617  ED A0               		ldi				; transfer last byte (length was stored length-2)
     226.  00:6619                      		
     227.  00:6619  E1                  		pop	hl			; address compressed data back from stack
     228.  00:661A                      	    
     229.  00:661A                      		IFDEF	BITBUSTER_OPTIMIZE_SPEED
     230.  00:661A                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     231.  00:661A                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     232.  00:661A                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     233.  00:661A                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     234.  00:661A                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     235.  00:661A                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     236.  00:661A                    ~ 		ENDIF
     237.  00:661A                      	
     238.  00:661A  18 B2               		jp	depack_loop
     239.  00:661C                      	
     240.  00:661C                      		ENDMODULE
