      43.  00:64C7                      ; DEFINE:	BITBUSTER_OPTIMIZE_SPEED
      44.  00:64C7                      ;	Defining this will optimize the bitbuster depacker for speed, at the cost of
      45.  00:64C7                      ;	bigger code.
      46.  00:64C7                      
      47.  00:64C7                      
      48.  00:64C7                      		IFDEF	BITBUSTER_OPTIMIZE_SPEED
