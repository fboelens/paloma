     113.  00:7ACB                      	;--- SPEED EQUALIZATION 
     114.  00:7ACB  3A 32 C0            	ld	a,(equalization_cnt)  		; if NTSC call 5 times out of 6
     115.  00:7ACE  FE 06               	cp	6					; if previously set on 6 then skip
