      79.  00:7AAD                      ttsfx_start:
      80.  00:7AAD                      	;--- Start a new SFX
      81.  00:7AAD                      	;--- Input B -> sfx number
      82.  00:7AAD                      	;--- Input C -> priority  0 = highest, 254 is lowest
      83.  00:7AAD                      
      84.  00:7AAD                      	;--- Test priority
      85.  00:7AAD  3A C7 C1            	ld	a,(sfx_PRIORITY)		; a:=Current sfx_SCC stream priority
      86.  00:7AB0  B9                  	cp	c				; If new sfx_SCC stream priority is higher than currently one...
      87.  00:7AB1  D8                  	ret	c				; ...we don't start the new sfx_SCC stream	
      88.  00:7AB2                      
      89.  00:7AB2                      	;--- Set priority
      90.  00:7AB2  79                  	ld	a,c				; a:=New priority
      91.  00:7AB3  32 C7 C1            	ld	(sfx_PRIORITY),a		; new priority saved in RAM
      92.  00:7AB6  3E 01               	ld	a,1		
      93.  00:7AB8  32 C6 C1            	ld	(sfx_STATUS),a		; playing SFX
      94.  00:7ABB                      
      95.  00:7ABB                      
      96.  00:7ABB  11 83 CE            	ld	de, sfxbank+1		; Start of SFX offset list
      97.  00:7ABE  68                  	ld	l, b
      98.  00:7ABF  26 00               	ld	h, 0
      99.  00:7AC1  29                  	add	hl, hl			; *2 to get item offset in bank
     100.  00:7AC2  19                  	add	hl, de
     101.  00:7AC3                      
     102.  00:7AC3  4E                  	ld	c,(hl)
     103.  00:7AC4  23                  	inc	hl
     104.  00:7AC5  46                  	ld	b,(hl)
     105.  00:7AC6  09                  	add	hl,bc				; Pointer to the first sfx
     106.  00:7AC7  22 C8 C1            	ld	(sfx_PSG_POINTER),hl
     107.  00:7ACA  C9                  	ret
     108.  00:7ACB                      
