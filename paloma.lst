     110.  00:79CB                      ttsfx_play:
     111.  00:79CB                      	;--- Disable this section if the SFX do not need to run at 
     112.  00:79CB                      	;    the same speed on 50hz and 60hz.
     113.  00:79CB                      	;--- SPEED EQUALIZATION 
     114.  00:79CB  3A 32 C0            	ld	a,(equalization_cnt)  		; if NTSC call 5 times out of 6
     115.  00:79CE  FE 06               	cp	6					; if previously set on 6 then skip
     116.  00:79D0  C8                  	ret	z
