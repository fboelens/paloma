      96.  00:64CD                      ; FUNCTION:	depack
      97.  00:64CD                      ;	Depack a blob of data that was packed with Bitbuster.
      98.  00:64CD                      ;
      99.  00:64CD                      ; ENTRY:
     100.  00:64CD                      ;	HL - Address of packed data
     101.  00:64CD                      ;	DE - Address to depack to
     102.  00:64CD                      ;   A - Start decompressing new data (0) or continue decompressing (<>0)
     103.  00:64CD                      ;
     104.  00:64CD                      ; EXIT:
     105.  00:64CD                      ;	HL - Size of depacked data
     106.  00:64CD                      ;	A  - Number of blocks left to decompress
     107.  00:64CD                      ;	FZ - Last block has been decompressed
     108.  00:64CD                      ;	FNZ- Last block hasn't been decompressed
     109.  00:64CD                      ;
     110.  00:64CD                      ; MODIFIES:
     111.  00:64CD                      ;	#AF, BC, DE, HL#
     112.  00:64CD                      ;
     113.  00:64CD                      depack:
     114.  00:64CD                      		
     115.  00:64CD  B7                  		or	a
     116.  00:64CE  20 02               		jr	nz,depack_next_block	; continue depacking a block if more blocks left
     117.  00:64D0                      		
     118.  00:64D0  7E                  		ld	a,(hl)		; get the number of blocks
     119.  00:64D1                      		
     120.  00:64D1  23                  		inc	hl			; move to size of first block
     121.  00:64D2                      		
     122.  00:64D2                      depack_next_block:
     123.  00:64D2  23                  		inc	hl
     124.  00:64D3  23                  		inc	hl          	; move over block size
     125.  00:64D4                      
     126.  00:64D4  F5                  		push af				; push number of blocks
     127.  00:64D5  D5                  		push de				; push destination address to be able to calculate decompressed size later on
     128.  00:64D6                      		
     129.  00:64D6  CD E4 64            		call depack_block	; decompress the block
     130.  00:64D9                      
     131.  00:64D9  EB                  		ex de,hl			; DE -> pointer to next block, HL -> pointer to first byte after decompressed data
     132.  00:64DA  C1                  		pop bc				; pop the original destination address
     133.  00:64DB  B7                  		or a				; clear carry
     134.  00:64DC  ED 42               		sbc hl,bc			; calculate length of decompressed data
     135.  00:64DE  44                  		ld b,h
     136.  00:64DF  4D                  		ld c,l				; put it in BC
     137.  00:64E0  EB                  		ex de,hl			; HL -> pointer to next block
     138.  00:64E1                      
     139.  00:64E1  F1                  		pop af				; pop number of blocks
     140.  00:64E2  3D                  		dec a				; one less to decompress
     141.  00:64E3  C9                  		ret
     142.  00:64E4                      
