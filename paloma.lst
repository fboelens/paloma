      79.  00:79AD                      ttsfx_start:
      80.  00:79AD                      	;--- Start a new SFX
      81.  00:79AD                      	;--- Input B -> sfx number
      82.  00:79AD                      	;--- Input C -> priority  0 = highest, 254 is lowest
      83.  00:79AD                      
      84.  00:79AD                      	;--- Test priority
      85.  00:79AD  3A C7 C1            	ld	a,(sfx_PRIORITY)		; a:=Current sfx_SCC stream priority
      86.  00:79B0  B9                  	cp	c				; If new sfx_SCC stream priority is higher than currently one...
      87.  00:79B1  D8                  	ret	c				; ...we don't start the new sfx_SCC stream	
      88.  00:79B2                      
      89.  00:79B2                      	;--- Set priority
      90.  00:79B2  79                  	ld	a,c				; a:=New priority
      91.  00:79B3  32 C7 C1            	ld	(sfx_PRIORITY),a		; new priority saved in RAM
      92.  00:79B6  3E 01               	ld	a,1		
      93.  00:79B8  32 C6 C1            	ld	(sfx_STATUS),a		; playing SFX
      94.  00:79BB                      
      95.  00:79BB                      
      96.  00:79BB  11 E5 CB            	ld	de, sfxbank+1		; Start of SFX offset list
      97.  00:79BE  68                  	ld	l, b
      98.  00:79BF  26 00               	ld	h, 0
      99.  00:79C1  29                  	add	hl, hl			; *2 to get item offset in bank
     100.  00:79C2  19                  	add	hl, de
     101.  00:79C3                      
     102.  00:79C3  4E                  	ld	c,(hl)
     103.  00:79C4  23                  	inc	hl
     104.  00:79C5  46                  	ld	b,(hl)
     105.  00:79C6  09                  	add	hl,bc				; Pointer to the first sfx
     106.  00:79C7  22 C8 C1            	ld	(sfx_PSG_POINTER),hl
     107.  00:79CA  C9                  	ret
