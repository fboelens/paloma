     134.  00:7ADE  2A C8 C1            	ld	hl,(sfx_PSG_POINTER)	; Pointer to the current sfx_PSG stream
     135.  00:7AE1  4E                  	ld	c,(hl)			; c:=Control byteb
     136.  00:7AE2  3A 72 C1            	ld	a,(PSG_regMIXER)
     137.  00:7AE5  47                  	ld	b,a				; b = mixer 
     138.  00:7AE6  23                  	inc	hl				; Increment pointer
     139.  00:7AE7                      
     140.  00:7AE7                      _ayCHECK_NT:
     141.  00:7AE7                      	; --- Check if there's new tone on stream ---
     142.  00:7AE7  CB 69               	bit	5,c				; If bit 5 c is off...
     143.  00:7AE9  28 08               	jp	z,_ayCHECK_NN		; ...jump to _ayCHECK_NN (no new tone)
     144.  00:7AEB                      	; --- Extract new tone from stream ---
     145.  00:7AEB  5E                  	ld	e,(hl)			; e:=lower byte of new tone
     146.  00:7AEC  23                  	inc	hl				; Increment pointer
     147.  00:7AED  56                  	ld	d,(hl)			; d:=higher byte of new tone
     148.  00:7AEE  23                  	inc	hl				; Increment pointer
     149.  00:7AEF  ED 53 CA C1         	ld	(sfx_PSG_TONE),de		; sfx_PSG tone updated
     150.  00:7AF3                      
     151.  00:7AF3                      _ayCHECK_NN:	
     152.  00:7AF3                      	; --- Check if there's new noise on stream ---
     153.  00:7AF3  CB 71               	bit	6,c				; if bit 6 c is off...
     154.  00:7AF5  28 09               	jp	z,_aySETPOINTER		; ...jump to _aySETPOINTER (no new noise)
     155.  00:7AF7                      	; --- Extract new noise from stream ---
     156.  00:7AF7  7E                  	ld	a,(hl)			; a:=New noise
     157.  00:7AF8  23                  	inc	hl				; Increment pointer
     158.  00:7AF9  FE 20               	cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
     159.  00:7AFB  28 44               	jp	z,_ayEND
     160.  00:7AFD                      _ayNOISE:
     161.  00:7AFD  32 CC C1            	ld	(sfx_PSG_NOISE),a		; sfx_PSG noise updated
     162.  00:7B00                      _aySETPOINTER:
     163.  00:7B00                      	; --- Update sfx_PSG pointer ---
     164.  00:7B00  22 C8 C1            	ld	(sfx_PSG_POINTER),hl	; Update sfx_PSG stream pointer
     165.  00:7B03                      
     166.  00:7B03                      	; --- Set noise register if enabled ---
     167.  00:7B03  CB 79               	bit	7,c				; If noise is off...
     168.  00:7B05  20 0B               	jp	nz,_ayMUSNOISE		; ...jump to _aySETMASKS
     169.  00:7B07  78                  	ld	a,b				; mixer mask for no music noise
     170.  00:7B08  F6 B0               	or	PSG_mixerOR
     171.  00:7B0A  E6 B7               	and	PSG_mixerAND
     172.  00:7B0C  47                  	ld	b,a
     173.  00:7B0D  3A CC C1            	ld	a,(sfx_PSG_NOISE)		; sfx_PSG noise value
     174.  00:7B10  18 03               	jp	_ayNOISESET
     175.  00:7B12                      _ayMUSNOISE:
     176.  00:7B12                      	;-- If no sfx noise then use music noise value
     177.  00:7B12  3A 79 C1            	ld	a,(replay_noise)		; replaer PSG noise value
     178.  00:7B15                      _ayNOISESET:
     179.  00:7B15  32 71 C1            	ld	(PSG_regNOISE),a		; copied in to AYREGS (noise channel)
     180.  00:7B18                      
     181.  00:7B18                      	; --- Extract and set volume if tone or noise is enabled ---
     182.  00:7B18  CB 61               	bit	4,c				; If tone is off...
     183.  00:7B1A  20 08               	jp	nz,_ayVOLUME		; ...jump to _ayMIXER
     184.  00:7B1C  2A CA C1            	ld	hl,(sfx_PSG_TONE)
     185.  00:7B1F  22 6B C1            	ld	(PSG_regTonereg),hl
     186.  00:7B22  CB 80               	res	PSG_tonebit,b				; enable tone on chan 
     187.  00:7B24                      
     188.  00:7B24                      _ayVOLUME:	
     189.  00:7B24                      	;--- Only update volume if tone or noise is active
     190.  00:7B24  3E 89               	ld	a,PSG_volumeAND
     191.  00:7B26  A0                  	and	b
     192.  00:7B27  FE 89               	cp	PSG_volumeCP
     193.  00:7B29  28 11               	jp	z,_ayMIXER
     194.  00:7B2B                      
     195.  00:7B2B  79                  	ld	a,c				; a:=Control byte
     196.  00:7B2C  E6 0F               	and	$0F				; lower nibble
     197.  00:7B2E                      	; --- Fix the volume using TT Volume Table ---
     198.  00:7B2E  2A CE C1            	ld	hl,(sfx_PSG_BALANCE)	; hl:=Pointer to relative volume table
     199.  00:7B31  85                  	add	a,l
     200.  00:7B32  6F                  	ld	l,a
     201.  00:7B33  30 01               	jp	nc,.skip
     202.  00:7B35  24                  	inc	h
     203.  00:7B36                      .skip:	
     204.  00:7B36  7E                  	ld	a,(hl)			; a:=sfx_PSG relative volume
     205.  00:7B37  E6 0F               	and	$0f				; mask only ay volume
     206.  00:7B39  32 73 C1            	ld	(PSG_regVOLreg),a		; Diretly update the TT register value
     207.  00:7B3C                      
     208.  00:7B3C                      _ayMIXER:
     209.  00:7B3C                      	;--- Set Mixer
     210.  00:7B3C  78                  	ld	a,b
     211.  00:7B3D  32 72 C1            	ld	(PSG_regMIXER),a		; Directly write the TT register value
     212.  00:7B40  C9                  	ret
     213.  00:7B41                      
     214.  00:7B41                      
     215.  00:7B41                      _ayEND:		
     216.  00:7B41  AF                  	xor   a		            ; remove bit 0
     217.  00:7B42  32 C6 C1            	ld	(sfx_STATUS),a
     218.  00:7B45  3D                  	dec	a				; reset prio (255)
     219.  00:7B46  32 C7 C1            	ld	(sfx_PRIORITY),a	
