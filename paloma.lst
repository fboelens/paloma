     120.  00:79D1                      	; Play sfx stream
     121.  00:79D1                      	;--------------------------------
     122.  00:79D1  3A C6 C1            	ld	a,(sfx_STATUS)		; a:=Current sfx_PSG stream priority
     123.  00:79D4  A7                  	and	a				; PSG active?
     124.  00:79D5  20 07               	jp	nz,.psg_play
     125.  00:79D7                      	
     126.  00:79D7                      	;--- If not active just use re-player noise and mixer
     127.  00:79D7  3A 79 C1            	ld	a,(replay_noise)
     128.  00:79DA  32 71 C1            	ld	(PSG_regNOISE),a
     129.  00:79DD  C9                  	ret
