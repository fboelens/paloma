     131.  00:79DE                      .psg_play:
     132.  00:79DE                      	; --- PLAY A FRAME OF AN sfx_PSG STREAM ---
     133.  00:79DE                      	; --- Extract control byte from stream ---
     134.  00:79DE  2A C8 C1            	ld	hl,(sfx_PSG_POINTER)	; Pointer to the current sfx_PSG stream
     135.  00:79E1  4E                  	ld	c,(hl)			; c:=Control byteb
     136.  00:79E2  3A 72 C1            	ld	a,(PSG_regMIXER)
     137.  00:79E5  47                  	ld	b,a				; b = mixer 
     138.  00:79E6  23                  	inc	hl				; Increment pointer
     139.  00:79E7                      
     140.  00:79E7                      _ayCHECK_NT:
     141.  00:79E7                      	; --- Check if there's new tone on stream ---
     142.  00:79E7  CB 69               	bit	5,c				; If bit 5 c is off...
     143.  00:79E9  28 08               	jp	z,_ayCHECK_NN		; ...jump to _ayCHECK_NN (no new tone)
     144.  00:79EB                      	; --- Extract new tone from stream ---
     145.  00:79EB  5E                  	ld	e,(hl)			; e:=lower byte of new tone
     146.  00:79EC  23                  	inc	hl				; Increment pointer
     147.  00:79ED  56                  	ld	d,(hl)			; d:=higher byte of new tone
     148.  00:79EE  23                  	inc	hl				; Increment pointer
     149.  00:79EF  ED 53 CA C1         	ld	(sfx_PSG_TONE),de		; sfx_PSG tone updated
     150.  00:79F3                      
     151.  00:79F3                      _ayCHECK_NN:	
     152.  00:79F3                      	; --- Check if there's new noise on stream ---
     153.  00:79F3  CB 71               	bit	6,c				; if bit 6 c is off...
     154.  00:79F5  28 09               	jp	z,_aySETPOINTER		; ...jump to _aySETPOINTER (no new noise)
     155.  00:79F7                      	; --- Extract new noise from stream ---
     156.  00:79F7  7E                  	ld	a,(hl)			; a:=New noise
     157.  00:79F8  23                  	inc	hl				; Increment pointer
     158.  00:79F9  FE 20               	cp	$20				; If it's an illegal value of noise (used to mark end of stream)...
