      20.  00:65AF                      		MODULE	bitbuster
      21.  00:65AF                      
      22.  00:65AF                      ; File: bitbuster
      23.  00:65AF                      ;
      24.  00:65AF                      ;	The bitbuster module gives you depack support for data packed
      25.  00:65AF                      ;	using the bitbuster algorithm.
      26.  00:65AF                      ;
      27.  00:65AF                      ; FUNCTION NAMES:
      28.  00:65AF                      ;	The functions <depack> and <depack_block> are MODULE local, which means you
      29.  00:65AF                      ;	have to add the prefix "bitbuster." to its name. This is to prevent possible
      30.  00:65AF                      ;	name clashes with functions from other libraries you might be using. 
      31.  00:65AF                      ;	However, if you define <MAKE_BITBUSTER_GLOBAL> then these functions will be
      32.  00:65AF                      ;	available without the "bitbuster." prefix as well.
      33.  00:65AF                      ;
      34.  00:65AF                      ; COPYRIGHT & CREDITS:
      35.  00:65AF                      ;	This module has been released by Arjan Bakker under the MIT License;
      36.  00:65AF                      ;	please see the top of the source file(s) for the full copyright statement.
      37.  00:65AF                      	
      38.  00:65AF                      ; DEFINE:	MAKE_BITBUSTER_GLOBAL
      39.  00:65AF                      ;	Defining this will make all public functions that are normally only
      40.  00:65AF                      ;	available with the "bitbuster." prefix to also be available without
      41.  00:65AF                      ;	this prefix. See the introduction of <bitbuster> for more information.
      42.  00:65AF                       
      43.  00:65AF                      ; DEFINE:	BITBUSTER_OPTIMIZE_SPEED
      44.  00:65AF                      ;	Defining this will optimize the bitbuster depacker for speed, at the cost of
      45.  00:65AF                      ;	bigger code.
      46.  00:65AF                      
      47.  00:65AF                      
      48.  00:65AF                      		IFDEF	BITBUSTER_OPTIMIZE_SPEED
      49.  00:65AF                    ~ ; use macro's for getbit routines when optimizing bitbuster depacker for speed 
      50.  00:65AF                    ~ ; (inlined code, no overhead by calls)
      51.  00:65AF                    ~ 
      52.  00:65AF                    ~ 
      53.  00:65AF                    ~ 
      54.  00:65AF                    ~ ; macro to get a bit from the bitstream
      55.  00:65AF                    ~ ; carry if bit is set, nocarry if bit is clear
      56.  00:65AF                    ~ 		MACRO	GET_BIT_FROM_BITSTREAM
      57.  00:65AF                    ~ 		add	a,a			; shift out new bit
      58.  00:65AF                    ~ 		jp nz,.done		; if remaining value isn't zere, we're done
      59.  00:65AF                    ~ 
      60.  00:65AF                    ~ 		ld a,(hl)		; get 8 bits from bitstream
      61.  00:65AF                    ~ 		inc	hl			; increase source data address
      62.  00:65AF                    ~ 
      63.  00:65AF                    ~ 		rla				; (bit 0 will be set!!!!)
      64.  00:65AF                    ~ .done:          
      65.  00:65AF                    ~ 		ENDM
      66.  00:65AF                    ~ 
      67.  00:65AF                    ~ 		ENDIF			; IFDEF	BITBUSTER_OPTIMIZE_SPEED
      68.  00:65AF                      	
      69.  00:65AF                      	
      70.  00:65AF                      		IFNDEF	BITBUSTER_OPTIMIZE_SPEED
      71.  00:65AF                      ; use calls for getbit code when not optimizing	for speed
      72.  00:65AF                      
      73.  00:65AF                      
      74.  00:65AF                      ; macro to get a bit from the bitstream
      75.  00:65AF                      ; carry if bit is set, nocarry if bit is clear
      76.  00:65AF                      		MACRO	GET_BIT_FROM_BITSTREAM
      77.  00:65AF                    < 		call	get_bit_from_bitstream
      78.  00:65AF                    < 		ENDM
      79.  00:65AF                      
      80.  00:65AF                      
      81.  00:65AF                      ; get a bit from the bitstream
      82.  00:65AF                      ; carry if bit is set, nocarry if bit is clear
      83.  00:65AF                      get_bit_from_bitstream:
      84.  00:65AF  87                  		add	a,a		; shift out new bit
      85.  00:65B0  C0                  		ret	nz		; if remaining value isn't zere, we're done
      86.  00:65B1                      	    
      87.  00:65B1  7E                  		ld a,(hl)	; get 8 bits from bitstream
      88.  00:65B2  23                  		inc hl		; increase source data address
      89.  00:65B3                      	
      90.  00:65B3  17                  		rla         ;(bit 0 will be set!!!!)
      91.  00:65B4  C9                  		ret
      92.  00:65B5                      
      93.  00:65B5                      		ENDIF		; IFNDEF BITBUSTER_OPTIMIZE_SPEED
