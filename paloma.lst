     144.  00:64E4                      ; FUNCTION:	depack_block
     145.  00:64E4                      ;	Depack data that was packed with Bitbuster.
     146.  00:64E4                      ;	Decompresses the RAW data, i.e. the data that is stored after the block count and block size!
     147.  00:64E4                      ;
     148.  00:64E4                      ; ENTRY:
     149.  00:64E4                      ;	HL - Address of packed data
     150.  00:64E4                      ;	DE - Address to depack to
     151.  00:64E4                      ;
     152.  00:64E4                      ; EXIT:
     153.  00:64E4                      ;	HL - Address of first byte after compressed data
     154.  00:64E4                      ;	DE - Address of first byte after decompressed data
     155.  00:64E4                      ;
     156.  00:64E4                      ; MODIFIES:
     157.  00:64E4                      ;	#AF, BC, BC', DE, DE', HL, HL'#
     158.  00:64E4                      ;
     159.  00:64E4                      depack_block:
     160.  00:64E4                      			
     161.  00:64E4                                      
     162.  00:64E4  3E 80                       ld	a,128
     163.  00:64E6                      
     164.  00:64E6                      depack_loop:    GET_BIT_FROM_BITSTREAM		; get compression type bit
     164.  00:64E6  CD C7 64          >   call get_bit_from_bitstream
     165.  00:64E9  38 04               		jp	c,output_compressed	; if set, we got lz77 compression
     166.  00:64EB  ED A0               		ldi				; copy byte from compressed data to destination (literal byte)
     167.  00:64ED                      	
     168.  00:64ED                      		IFDEF	BITBUSTER_OPTIMIZE_SPEED
     169.  00:64ED                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     170.  00:64ED                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     171.  00:64ED                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     172.  00:64ED                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     173.  00:64ED                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     174.  00:64ED                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     175.  00:64ED                    ~ 		ENDIF
     176.  00:64ED                      
     177.  00:64ED  18 F7               		jp	depack_loop
     178.  00:64EF                      	
     179.  00:64EF                      
     180.  00:64EF                      ;handle compressed data
     181.  00:64EF                      output_compressed:
     182.  00:64EF                      ; calculate length value
     183.  00:64EF  01 01 00            		ld	bc,1
     184.  00:64F2                      get_gamma_value:
     185.  00:64F2                      	  	GET_BIT_FROM_BITSTREAM   	; get more bits
     185.  00:64F2  CD C7 64          >   call get_bit_from_bitstream
     186.  00:64F5  30 0A               	  	jr 	nc,get_gamma_value_end
     187.  00:64F7                      	  	GET_BIT_FROM_BITSTREAM   	; get next bit of value from bitstream
     187.  00:64F7  CD C7 64          >   call get_bit_from_bitstream
     188.  00:64FA  CB 11               	  	rl	c
     189.  00:64FC  CB 10               	  	rl	b
     190.  00:64FE  30 F2               	  	jp 	nc,get_gamma_value  	; repeat unless overflow occurred (=end of block marker)
     191.  00:6500  C9                  	  	ret
     192.  00:6501                        	
     193.  00:6501                      get_gamma_value_end:
     194.  00:6501  FD 60               		ld iyh,b
     195.  00:6503  FD 69               		ld iyl,c
     196.  00:6505                      
     197.  00:6505  4E                  		ld	c,(hl)			; get lowest 7 bits of offset, plus offset extension bit
     198.  00:6506  23                  		inc	hl			; to next byte in compressed data
     199.  00:6507                      	  	
     200.  00:6507  06 00               		ld	b,0
     201.  00:6509  CB 79               		bit	7,c
     202.  00:650B  28 16               		jr	z,output_match1		; no need to get extra bits if carry not set
     203.  00:650D                      	
     204.  00:650D                      		GET_BIT_FROM_BITSTREAM		; get offset bit 10 
     204.  00:650D  CD C7 64          >   call get_bit_from_bitstream
     205.  00:6510  CB 10               		rl	b
     206.  00:6512                      		GET_BIT_FROM_BITSTREAM		; get offset bit 9
     206.  00:6512  CD C7 64          >   call get_bit_from_bitstream
     207.  00:6515  CB 10               		rl	b
     208.  00:6517                      		GET_BIT_FROM_BITSTREAM		; get offset bit 8
     208.  00:6517  CD C7 64          >   call get_bit_from_bitstream
     209.  00:651A  CB 10               		rl	b
     210.  00:651C                      		GET_BIT_FROM_BITSTREAM		; get offset bit 7
     210.  00:651C  CD C7 64          >   call get_bit_from_bitstream
     211.  00:651F                      
     212.  00:651F  38 03               		jp	c,output_match2		; since extension mark already makes bit 7 set
     213.  00:6521  CB B9               		res	7,c			; only clear it if the bit should be cleared
     214.  00:6523  37                  output_match1:  scf
     215.  00:6524  E5                  output_match2:	push	hl			; address compressed data on stack
     216.  00:6525                      
     217.  00:6525  62                  		ld	h,d
     218.  00:6526  6B                  		ld	l,e			; destination address in HL...
     219.  00:6527  ED 42               		sbc	hl,bc		; calculate source address, keep the required CF in mind! (length was stored length-2)
     220.  00:6529                      
     221.  00:6529  FD 44               		ld b,iyh
     222.  00:652B  FD 4D               		ld c,iyl
     223.  00:652D                      	
     224.  00:652D  ED B0               		ldir			; transfer data
     225.  00:652F  ED A0               		ldi				; transfer last byte (length was stored length-2)
     226.  00:6531                      		
     227.  00:6531  E1                  		pop	hl			; address compressed data back from stack
     228.  00:6532                      	    
     229.  00:6532                      		IFDEF	BITBUSTER_OPTIMIZE_SPEED
     230.  00:6532                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     231.  00:6532                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     232.  00:6532                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     233.  00:6532                    ~ 		GET_BIT_FROM_BITSTREAM		; get compression type bit
     234.  00:6532                    ~ 		jp	c,output_compressed	; if set, we got lz77 compression
     235.  00:6532                    ~ 		ldi				; copy byte from compressed data to destination (literal byte)
     236.  00:6532                    ~ 		ENDIF
     237.  00:6532                      	
     238.  00:6532  18 B2               		jp	depack_loop
     239.  00:6534                      	
     240.  00:6534                      		ENDMODULE
