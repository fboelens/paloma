      50.  00:64C7                    ~ ; (inlined code, no overhead by calls)
      51.  00:64C7                    ~ 
      52.  00:64C7                    ~ 
      53.  00:64C7                    ~ 
      54.  00:64C7                    ~ ; macro to get a bit from the bitstream
      55.  00:64C7                    ~ ; carry if bit is set, nocarry if bit is clear
      56.  00:64C7                    ~ 		MACRO	GET_BIT_FROM_BITSTREAM
      57.  00:64C7                    ~ 		add	a,a			; shift out new bit
      58.  00:64C7                    ~ 		jp nz,.done		; if remaining value isn't zere, we're done
      59.  00:64C7                    ~ 
      60.  00:64C7                    ~ 		ld a,(hl)		; get 8 bits from bitstream
      61.  00:64C7                    ~ 		inc	hl			; increase source data address
      62.  00:64C7                    ~ 
      63.  00:64C7                    ~ 		rla				; (bit 0 will be set!!!!)
      64.  00:64C7                    ~ .done:          
      65.  00:64C7                    ~ 		ENDM
      66.  00:64C7                    ~ 
      67.  00:64C7                    ~ 		ENDIF			; IFDEF	BITBUSTER_OPTIMIZE_SPEED
