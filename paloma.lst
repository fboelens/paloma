      85.  00:64C8  C0                  		ret	nz		; if remaining value isn't zere, we're done
      86.  00:64C9                      	    
      87.  00:64C9  7E                  		ld a,(hl)	; get 8 bits from bitstream
      88.  00:64CA  23                  		inc hl		; increase source data address
      89.  00:64CB                      	
      90.  00:64CB  17                  		rla         ;(bit 0 will be set!!!!)
      91.  00:64CC  C9                  		ret
      92.  00:64CD                      
