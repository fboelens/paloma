      36.  00:7A8A                      ; ---	ttsfx_init
      37.  00:7A8A                      ; Initialise the sfx player. Sets initial priorities, volume
      38.  00:7A8A                      ; balancce and initial SCC waveform.
      39.  00:7A8A                      ;
      40.  00:7A8A                      ;===========================================================	
      41.  00:7A8A                      ttsfx_init:
      42.  00:7A8A  3E 0F               	ld	a,15
      43.  00:7A8C  CD 98 7A            	call	ttsfx_psg_balance	
      44.  00:7A8F  AF                  	xor	a
      45.  00:7A90  32 C6 C1            	ld	(sfx_STATUS),a
      46.  00:7A93  3D                  	dec	a
      47.  00:7A94  32 C7 C1            	ld	(sfx_PRIORITY),a
      48.  00:7A97  C9                  	ret
      49.  00:7A98                      
      50.  00:7A98                      ;===========================================================
      51.  00:7A98                      ; ---	ttsfx_psg_balance
      52.  00:7A98                      ; Set the main SFXvolume for the PSG chip.
      53.  00:7A98                      ;
      54.  00:7A98                      ; in: [A] master volume (0-15) 0=silenced, 15=full volume. 
      55.  00:7A98                      ;===========================================================	
      56.  00:7A98                      ttsfx_psg_balance:
      57.  00:7A98  CD 9F 7A            	call	_getsfxbalancebase
      58.  00:7A9B  22 CE C1            	ld	(sfx_PSG_BALANCE),hl	
      59.  00:7A9E  C9                  	ret				
      60.  00:7A9F                      				
      61.  00:7A9F                      
      62.  00:7A9F                      _getsfxbalancebase:
      63.  00:7A9F  07                  	rlca
      64.  00:7AA0  07                  	rlca
      65.  00:7AA1  07                  	rlca
      66.  00:7AA2  07                  	rlca
      67.  00:7AA3  21 CA 76            	ld	hl,_VOLUME_TABLE_PSG			; Volume table from TT replayer
      68.  00:7AA6  E6 F0               	and	$f0
      69.  00:7AA8  85                  	add	a,l
      70.  00:7AA9  6F                  	ld	l,a
      71.  00:7AAA  D0                  	ret 	nc
      72.  00:7AAB  24                  	inc	h
      73.  00:7AAC  C9                  	ret			
